                                                    <font>
                                                        <Font name="System Bold" size="14.0" />
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="1.0" />
                                                    </VBox.margin>
                                                </JFXButton>
                                                <JFXButton fx:id="btnWrong" onAction="#btnWrongAction" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #00beff;" text="好" textFill="WHITE">
