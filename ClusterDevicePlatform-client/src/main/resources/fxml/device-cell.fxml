<fx:root prefHeight="118.0" prefWidth="143.0" type="StackPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <JFXTabPane>
            <tabs>
                <Tab fx:id="tabStatus" text="n号">
